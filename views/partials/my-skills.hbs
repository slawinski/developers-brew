{{!-- my-skills --}}
<section class="bg-2" id="my-skills">
  <div class="container-fluid">
    <div class="text-center">
      <h2 class="section-heading text-uppercase">Our beers</h2>
      <h3 class="section-subheading text-muted">Here are some of our finest beers</h3>
    </div>
    <div class="grid text-center">
      {{#each data}}
      <div>
        <img class="skill-img" src="{{this.img-address}}" alt="{{this.skill}}">
        <div>
          <h4>{{this.beer-pun}}</h4>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</section>
