{{!-- my-skills --}}
<section class="bg-2" id="my-skills">
  <div class="container-fluid">
    <div class="text-center">
      <h2 class="section-heading text-uppercase">Our beers</h2>
      <h3 class="section-subheading text-muted">Here are some of our finest beers</h3>
    </div>
    <div class="row text-center">
      {{#each data}}
      <div class="col-md-2 col-xs-4">
        <img class="img-fluid" src="{{this.img-address}}" alt="{{this.skill}}">
        <div>
          <p>{{this.beer-pun}}</p>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</section>
